    "Turns": [
      {
        "Proto": "udp",
        "URI": "turn:netbird.{{ internal_domain }}:3478",
        "Username": "self",
        "Password": "{{ vpn_turn_password }}"
      }
    ],
    "CredentialsTTL": "12h",
    "Secret": "secret",
    "TimeBasedCredentials": false
